{
  "family": "mercor-demo-task",
  "networkMode": "bridge",
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "REPLACE_EXEC_ROLE_ARN",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "REPLACE_IMAGE",
      "essential": true,
      "portMappings": [{"containerPort":8080,"hostPort":8080}],
      "environment": [{"name":"APP_MSG","value":"Hello from green!"}],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/mercor-demo",
          "awslogs-region": "REPLACE_REGION",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
