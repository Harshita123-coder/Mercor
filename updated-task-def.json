{
    "family": "mercor-demo-task",
    "executionRoleArn": "arn:aws:iam::533567530547:role/mercor-demo-task-exec",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "app",
            "image": "533567530547.dkr.ecr.us-east-1.amazonaws.com/mercor-ecs-demo:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "APP_MSG",
                    "value": "Hello from v4 (blue) - Fixed Container Issue!"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/mercor-demo",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "placementConstraints": [],
    "cpu": "256",
    "memory": "512"
}